{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],

  "routes": [
    {
      "src": "/(.*)",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "dest": "/server.js",
      "headers":{
        "Access-Control-Allow-Origin":"*"
      }
    }
  ]
}
