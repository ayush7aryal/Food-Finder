{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],

  "routes": [
    {
      "src": "/(.*)",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "dest": "/server.js",
      "headers":{
        "Access-Control-Allow-Origin":"*",
        "Access-Control-Allow-Credentials":"true",
        "Access-Control-Allow-Headers":"Access-Control-Allow-Headers"
      }
    },
    {
      "src": "/user/login",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "headers":{
        "Access-Control-Allow-Origin":"*",
        "Access-Control-Allow-Credentials":"true",
        "Access-Control-Allow-Headers":"Access-Control-Allow-Headers"
      }
    }
  ]
}
